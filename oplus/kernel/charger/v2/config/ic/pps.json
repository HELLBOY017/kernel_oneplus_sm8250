            "lable": "OPLUS_IC_FUNC_PPS_PDO_SET",
            "desc": {
                "zh": "设置PDO"
            },
            "auto_debug_code": true,
            "parameter_list": ["int", "int"],
            "parameter_desc": [
                {
                    "range": "0-*",
                    "type": "in",
                    "desc": {
                        "zh": "要设置的电压，单位mV"
                    }
                },
                {
                    "range": "0-*",
                    "type": "out",
                    "desc": {
                        "zh": "要设置的电流，单位mA"
                    }
                }
            ],
            "vict": {
                "read": false,
                "write": {
                    "cmd": "vict -f {id} -S{0},{1} vic-*",
                    "desc": {
                        "zh": "设置pdo, {0}为电压，{1}为电流"
                    }
                },
                "overwrite": {
                    "cmd": "vict -f {id} -O {0},{1} vic-*",
                    "desc": {
                        "zh": "覆盖pdo, {0}为电压，{1}为电流"
                    }
                }
            }
        },
