		        //qcom,min-dropout-voltage = <160000>;
		/*endif */
		};

		qcom,pm8008-l6@4400 {
			reg =  /bits/ 16 <0x4500>;
		/*#ifdef  */
		        regulator-min-microvolt = <2800000>;
		        regulator-max-microvolt = <2800000>;
		        //qcom,min-dropout-voltage = <184000>;
