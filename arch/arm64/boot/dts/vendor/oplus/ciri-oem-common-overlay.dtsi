		qcom,pm8008-l3@4200 {
			reg =  /bits/ 16 <0x4200>;
		        regulator-min-microvolt = <2800000>;
		        regulator-max-microvolt = <2800000>;
		/*#ifdef  */
		        //qcom,min-dropout-voltage = <136000>;
		/*endif */
