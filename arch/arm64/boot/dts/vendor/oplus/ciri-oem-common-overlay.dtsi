		qcom,pm8008-l7@4400 {
			reg =  /bits/ 16 <0x4600>;
		/*#ifdef  */
		        regulator-min-microvolt = <2900000>;
		        regulator-max-microvolt = <2900000>;
		        //qcom,min-dropout-voltage = <200000>;
		/*endif */
		};
