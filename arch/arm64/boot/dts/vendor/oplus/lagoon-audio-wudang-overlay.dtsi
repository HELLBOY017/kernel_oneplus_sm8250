
&tlmm {
		/* #ifdef OPLUS_ARCH_EXTENDS */
		mi2s_0_pins: mi2s_0_pins {
			mi2s_0_active: mi2s_0_active {
				mux {
					pins = "gpio88", "gpio89", "gpio90", "gpio91";
					function = "mi2s_0";
				};

				config {
					pins = "gpio88", "gpio89", "gpio90", "gpio91";
					drive-strength = <6>;
					bias-disable;
				};
			};

			mi2s_0_sleep: mi2s_0_sleep {
				mux {
					pins = "gpio88", "gpio89", "gpio90", "gpio91";
					function = "gpio";
				};

				config {
					pins = "gpio88", "gpio89", "gpio90", "gpio91";
					drive-strength = <2>;
					bias-pull-down;
				};
			};
		};
		//#endif /* OPLUS_ARCH_EXTENDS */
};

