	qcom,enable-qpnp-qg;
	qcom,qg-ext-sns;
	qcom,oplus_small_board_temp;
	qcom,battery-data = <&mtp_batterydata>;
	qcom,qg-iterm-ma = <260>;
	qcom,rbat-conn-mohm = <0>;
	qcom,qg-fast-chg-config;
	//qcom,hold-soc-while-full;
