&pm7250b_gpios {
	dischg_enable: dischg_enable{
		pins = "gpio7";
		function = "normal";
		qcom,drive-strength = <2>;
		power-source = <0>;
		bias-disable;
		output-high;
	};

	dischg_disable: dischg_disable{
		pins = "gpio7";
		function = "normal";
		qcom,drive-strength = <2>;
		power-source = <0>;
		bias-disable;
		output-low;
	};

    vooc_asic_id_active: vooc_asic_id_active {
		pins = "gpio5";
		function = "normal";
		bias-pull-up;
		input-enable;
		power-source = <1>;
	};

    vooc_asic_id_sleep: vooc_asic_id_sleep {
		pins = "gpio5";
		function = "normal";
		bias-pull-down;
		input-enable;
		power-source = <1>;
	};

	gpio1_adc {
        gpio1_adc_default: gpio1_adc_default {
        pins = "gpio1"; /* GPIO 1 */
        function = "normal"; /* normal */
        bias-high-impedance; /* DISABLE GPIO1 for ADC*/
        };
    };
};

